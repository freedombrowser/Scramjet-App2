#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/workspaces/Scramjet-App2/node_modules/.pnpm/@mercuryworkshop+wisp-js@0.4.1_utf-8-validate@6.0.4/node_modules/@mercuryworkshop/wisp-js/src/bin/node_modules:/workspaces/Scramjet-App2/node_modules/.pnpm/@mercuryworkshop+wisp-js@0.4.1_utf-8-validate@6.0.4/node_modules/@mercuryworkshop/wisp-js/src/node_modules:/workspaces/Scramjet-App2/node_modules/.pnpm/@mercuryworkshop+wisp-js@0.4.1_utf-8-validate@6.0.4/node_modules/@mercuryworkshop/wisp-js/node_modules:/workspaces/Scramjet-App2/node_modules/.pnpm/@mercuryworkshop+wisp-js@0.4.1_utf-8-validate@6.0.4/node_modules/@mercuryworkshop/node_modules:/workspaces/Scramjet-App2/node_modules/.pnpm/@mercuryworkshop+wisp-js@0.4.1_utf-8-validate@6.0.4/node_modules:/workspaces/Scramjet-App2/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/workspaces/Scramjet-App2/node_modules/.pnpm/@mercuryworkshop+wisp-js@0.4.1_utf-8-validate@6.0.4/node_modules/@mercuryworkshop/wisp-js/src/bin/node_modules:/workspaces/Scramjet-App2/node_modules/.pnpm/@mercuryworkshop+wisp-js@0.4.1_utf-8-validate@6.0.4/node_modules/@mercuryworkshop/wisp-js/src/node_modules:/workspaces/Scramjet-App2/node_modules/.pnpm/@mercuryworkshop+wisp-js@0.4.1_utf-8-validate@6.0.4/node_modules/@mercuryworkshop/wisp-js/node_modules:/workspaces/Scramjet-App2/node_modules/.pnpm/@mercuryworkshop+wisp-js@0.4.1_utf-8-validate@6.0.4/node_modules/@mercuryworkshop/node_modules:/workspaces/Scramjet-App2/node_modules/.pnpm/@mercuryworkshop+wisp-js@0.4.1_utf-8-validate@6.0.4/node_modules:/workspaces/Scramjet-App2/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@mercuryworkshop/wisp-js/src/bin/server_cli.mjs" "$@"
else
  exec node  "$basedir/../@mercuryworkshop/wisp-js/src/bin/server_cli.mjs" "$@"
fi
